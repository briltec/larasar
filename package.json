{
  "name": "Larasar",
  "version": "0.0.1",
  "description": "A Laravel + Quasar Vue Framework App",
  "productName": "Larasar",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Mohamed Dembele <modembfr@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "uy": "npm update && yarn && quasar build -m pwa && quasar dev",
    "uca": "npm update && quasar dev -m capacitor -T android",
    "u": "npm update && quasar build -m pwa && quasar dev",
    "y": "yarn && quasar build -m pwa && quasar dev",
    "uco": "npm update && quasar dev -m android",
    "ca": "quasar dev -m capacitor -T android",
    "b": "quasar build -m pwa && quasar dev",
    "d": "npm update && quasar dev",
    "co": "quasar dev -m android",
    "p": "quasar dev -m pwa"
  },
  "dependencies": {
    "@quasar/extras": "^1.9.18",
    "axios": "^0.25",
    "core-js": "^3.6.5",
    "laravel-echo": "^1.11.0",
    "pusher-js": "^7.0.3",
    "quasar": "^2.0.0",
    "timeago.js": "^4.0.2",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@inertiajs/inertia": "^0.10.0",
    "@inertiajs/inertia-vue3": "^0.5.1",
    "@inertiajs/progress": "^0.2.6",
    "@quasar/app": "^3.0.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vue/compiler-sfc": "^3.0.5",
    "alpinejs": "^2.8.1",
    "autoprefixer": "^10.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.0.0",
    "eslint-webpack-plugin": "^2.4.0",
    "laravel-jetstream": "^0.0.3",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14",
    "postcss-import": "^12.0.1",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.0",
    "vue-loader": "^16.1.2",
    "webpack-cli": "^4.7.2",
    "workbox-webpack-plugin": "^6.0.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
